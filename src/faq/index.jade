extends ../_layouts/base

include ../_modules/hero/hero
include ../_modules/faq/faq

block config
  //- Correct base url pathing for this page
  - var baseUrl = '../'

block content
    main.faqs.bg-secondary
        +hero(site.data.faq.hero.title, site.data.faq.hero.desc, site.data.faq.hero.img)
            each item in site.data.faq.list
                +faq(item)
